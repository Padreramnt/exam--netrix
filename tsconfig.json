{
	"compilerOptions": {
		"target": "ES2020",
		"module": "ES2020",
		"moduleResolution": "Node",
		"allowUnreachableCode": false,
		"allowUnusedLabels": false,
		"strict": true,
		"alwaysStrict": true,
		"noImplicitAny": true,
		"noImplicitReturns": true,
		"noFallthroughCasesInSwitch": true,
		"noErrorTruncation": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"importHelpers": true,
		"jsx": "react",
		"rootDir": "./src"
	}
}
